int countPrimes(int max) {
  if (max < 2) {
    return 0;
  }

  boolean[] isPrime = {};

  void printPrimes(String... args) {
    for(int i = 0;i<sizeof(isPrime);i++) {
      println(""+i+": "+isPrime[i]);
    }
  }

  for(int i = 0;i<max;i++) {
    isPrime[i] = false;
  }

  for (int i = 2; i <= max; i++) {
    isPrime[i] = true;
  }

  for (int p = 2; p * p <= max; p++) {
    if (isPrime[p]) {
      for (int i = p * p; i <= max; i += p) {
        isPrime[i] = false;
      }
    }
  }

  int primeCount = 0;
  for (int i = 2; i <= max; i++) {
    if (isPrime[i]) {
      primeCount++;
    }
  }
  printPrimes("Hallo", "Welt");
  return primeCount;
}

return countPrimes(100);
